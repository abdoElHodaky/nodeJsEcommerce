<table class="table table-striped table-inverse">
	<thead>
	<%for(var i in ComparedProducts ){%>
	<th>
		<img src="<%=ComparedProducts[i][0].ProductThumbnail%>" width="100" height="100">
		<h4 class="heading" ><%=ComparedProducts[i][0].ProductName%></h4>
	</th>
	<%}%>
	<tbody>
		<tr>
		<%for(var i in ComparedProducts){%>
		<td>
			<ul class="list-group">
				<h3 class="heading" >Specifications</h3>
				<li class="list-group-item row">
				<div class="row row col-xl-12">
					<%for(var j in ComparedProducts[i]){%>
					<div class="col-sm-10">
							<div class="col-sm-7">
							<h4 class="heading"><%=ComparedProducts[i][j].SpecName%></h4>
						</div>
						<div class="col-sm-3">
							<h5 class="sub-heading" ><%=ComparedProducts[i][j].SpecValue%></h5>
						</div>
					</div>
					<%}%>	
				</div>
				</li>
			</ul>
			<ul class="list-group">
					<h3 class="heading" >Details</h3>
					<li class="list-group-item row">
					<div class="row col-xl-12">
						<div class="col-sm-5 col-xs-3">
							<h4 class="sub-heading" >Desc</h4>
						</div>	
						<div class="col-sm-3">
						<h5 class="sub-heading"><%=ComparedProducts[i][0].ProductDesc%></h5>
						</div>
					</div>
					<div class="row col-xl-12">
						<div class="col-sm-5 col-xs-3">
							<h4 class="sub-heading" >Price</h4>
						</div>	
						<div class="col-sm-3">
						<h5 class="sub-heading">
							<%=ComparedProducts[i][0].Price%>
						</h5>
							
						</div>
					</div>
					<div class="row col-xl-12">
						<div class="col-sm-5 col-xs-3">
							<h4 class="sub-heading" >Cat</h4>
						</div>	
						<div class="col-sm-3">
						<h5 class="sub-heading">
							<%=ComparedProducts[i][0].Cat%>
						</h5>
						</div>
					</div>
					</li>
			</ul>
		</td>
		<%}%>
		</tr>
	</tbody>	
</table>